  "source": "pragma solidity ^0.4.19;\n\ncontract Versionable {\n    uint version;\n\n    function Versionable () public {\n        version = 1;\n    }\n\n    function bumpVersion () public {\n        version += 1;\n    }\n\n    function getCurrentVersion () public view returns (uint) {\n        return version;\n    }\n}",
